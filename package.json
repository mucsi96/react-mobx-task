{
  "name": "react-mobx-task",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "build": "parcel build index.html --no-cache",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "precommit": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mucsi96/react-mobx-task.git"
  },
  "author": "Igor Mucsicska <mucsi_96@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mucsi96/react-mobx-task/issues"
  },
  "homepage": "https://github.com/mucsi96/react-mobx-task#readme",
  "dependencies": {
    "mobx": "^5.5.1",
    "mobx-react": "^5.3.4",
    "page": "^1.10.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/page": "^1.8.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "parcel": "^1.10.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "react-testing-library": "^5.2.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3"
  }
}
